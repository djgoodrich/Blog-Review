<div class ="container">
{{> navbar/navbar}}
    <h1 class="mt-3 mb-3" id="userHead">{{user.name}}</h1>
    <h2 id="indentation">Reviews</h2>
    {{#each user.Reviews}}
        <div class="card mt-3">
            <div class="card-body" >
                <h3 id="title">{{this.title}}</h3>
                <!-- Need to also include Blog table -->
                <h4 id="underline"><em>{{this.Blog.title}}</em></h4>
                <p id="indent">{{this.body}}</p>
            </div>
        </div>
    {{/each}}
 </div>

 {{> scripts/scripts}}