
{{> layouts/navbar}}

<div class ="container">
    <h1 class="mt-3 mb-3">{{user.name}}</h1>
    <h2>Reviews</h2>
    {{#each user.Reviews}}
        <div class="card mt-3">
            <div class="card-body">
                <h3>{{this.title}}</h3>
                <!-- Need to also include Blog table -->
                <h4><em>Blog Title</em></h4>
                <p>{{this.body}}</p>
            </div>
        </div>
    {{/each}}
 </div>